{
  "name": "react-express",
  "version": "0.0.1",
  "description": "",
  "main": "dist/www.js",
  "scripts": {
    "mkconfig": "test -e config.json || cp config.template.json config.json",
    "clean:dist": "rm -rfv ./dist",
    "clean:db": "find db -type f -not -name '.keep' -exec rm -rfv {} \\;",
    "gulp:build": "tsc --target ES2015 --module commonjs --lib es6 --esModuleInterop --outDir gulpfile.js $(find ./gulpfile.ts/ -type f -name '*.ts')",
    "pregulp": "npm run gulp:build",
    "gulp": "gulp",
    "pretypeorm": "NODE_ENV=development npm run gulp -- build",
    "typeorm": "typeorm",
    "migration:generate": "npm run typeorm -- migration:generate -n Initialize",
    "migration:run": "npm run typeorm -- migration:run",
    "prebuild": "",
    "build": "NODE_ENV=production npm run gulp -- build",
    "prestart:dev": "npm run mkconfig",
    "start:dev": "NODE_ENV=development npm run gulp -- watch",
    "start": "",
    "test": "jest"
  },
  "author": "",
